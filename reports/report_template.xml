<?xml version="1.0" encoding="utf-8" ?>
<openerp>
    <data>
        <template id="report_project_task">
            <t t-call="report.html_container">
                <t t-foreach="docs" t-as="o">
                    <t t-call="report.external_layout">
                        <div class="page">
                            <h2>Report title</h2>
                            <p>This object's name is <span t-field="o.name"/></p>
                        </div>
                    </t>
                </t>
            </t>
        </template>
    </data>
</openerp>